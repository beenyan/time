{
  "name": "time",
  "version": "1.0.0",
  "description": "一個簡單的計時器",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
		"pack": "electron-builder --dir",
		"dist": "electron-builder" 
  },
  "build": {
		"appId": "time",
		"productName": "計時器",
		"directories": {
			"output": "pack"
		},
		"files": [
			"**/*", 
			"!.vscode",
			"build/"
		],
		"asar": false,  
		"win": {
		  	"icon": "assets/icon.png"
		},
		"mac": {
		  	"icon": "assets/icon.png"
		},
		"linux": {
		  	"icon": "assets/icon.png"
		}
	},
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "聖冰如焰",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^8.3.0",
    "electron-builder": "^22.6.1"
  },
  "dependencies": {
  }
}
